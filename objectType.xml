<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/objectType.xsd">
    <import>
        <!-- Tagging -->
        <type>
            <name>de.teralios.quizCreator.quiz</name>
            <definitionname>com.woltlab.wcf.tagging.taggableObject</definitionname>
            <classname>wcf\system\tagging\TaggableQuiz</classname>
        </type>

        <!-- BBCode/HTMLCode-Support for quiz. -->
        <type>
            <name>de.teralios.quizCreator.quiz</name>
            <definitionname>com.woltlab.wcf.message</definitionname>
        </type>

        <!-- Trophies -->
        <type>
            <name>de.teralios.quizCreator.played</name>
            <definitionname>com.woltlab.wcf.condition.trophy</definitionname>
            <classname>wcf\system\condition\UserIntegerPropertyCondition</classname>
            <conditiongroup>contents</conditiongroup>
            <propertyname>quizPlayed</propertyname>
            <minvalue>0</minvalue>
        </type>

        <type>
            <name>de.teralios.quizCreator.quizPlayedUnique</name>
            <definitionname>com.woltlab.wcf.condition.trophy</definitionname>
            <classname>wcf\system\condition\UserIntegerPropertyCondition</classname>
            <conditiongroup>contents</conditiongroup>
            <propertyname>quizPlayedUnique</propertyname>
            <minvalue>0</minvalue>
        </type>

        <type>
            <name>de.teralios.quizCreator.quizMaxScore</name>
            <definitionname>com.woltlab.wcf.condition.trophy</definitionname>
            <classname>wcf\system\condition\UserIntegerPropertyCondition</classname>
            <conditiongroup>contents</conditiongroup>
            <propertyname>quizMaxScore</propertyname>
            <minvalue>0</minvalue>
        </type>

        <!-- Category -->
        <type>
            <name>de.teralios.quizCreator.quiz.category</name>
            <definitionname>com.woltlab.wcf.category</definitionname>
            <classname>wcf\system\category\QuizCategoryType</classname>
        </type>

        <!-- box controllers -->
        <type>
            <name>de.teralios.quizCreator.category</name>
            <definitionname>com.woltlab.wcf.boxController</definitionname>
            <classname>wcf\system\box\QuizCategoriesBoxController</classname>
        </type>
        <type>
            <name>de.teralios.quizCreator.lastPlayers</name>
            <definitionname>com.woltlab.wcf.boxController</definitionname>
            <classname>wcf\system\box\QuizLastPlayersBoxController</classname>
        </type>
        <type>
            <name>de.teralios.quizCreator.bestPlayers</name>
            <definitionname>com.woltlab.wcf.boxController</definitionname>
            <classname>wcf\system\box\QuizBestPlayersBoxController</classname>
        </type>
        <type>
            <name>de.teralios.quizCreator.mostPlayed</name>
            <definitionname>com.woltlab.wcf.boxController</definitionname>
            <classname>wcf\system\box\QuizMostPlayedBoxController</classname>
        </type>

        <type>
            <name>de.teralios.quizCreator.userResult</name>
            <definitionname>com.woltlab.wcf.boxController</definitionname>
            <classname>wcf\system\box\QuizUserResultBoxController</classname>
        </type>
    </import>
</data>